	{
		var a = 5;
		var b:int;
		b= -a;
		return b;
	}

	static public fun test2():int
	{
		var a:int=5;
		++a;
		return a;
	}

	static public fun test3():int
	{
		var a:int=5;
		a=--a;
		return a;
	}

	static public fun test4():int
	{
		var a:int=5;
		a++;
		return a;
	}

	static public fun test5():int
	{
		var a:int=5;
		a=a--;
		return a;
	}

	static public fun test6():int
	{
		var a:int=5;
		var b:int=6;
		return a+b;
	}

	static public fun test7():long
	{
		var a=101;
		return a;
	}

	static public fun test8():double
	{
		var a=101;
		return a;
	}
