using fay.system;

package fay.dev.test;

public class FlowTest
{
	static public fun test1():int
	{
		var a=5, b=6, c=7;
		if(a>b)
			return c;

		return a;
	}

	static public fun test2():int
	{
		var a=5, b=6, c=7;
		var d:int;

		if(a>b)
			d=a;
		elseif(b<c)
			d=c;
		else
			d=b;

		return d;
	}

	static public fun test3():int
	{
		var count=0;
		for(var a=1; a<=10; ++a)
		{
			count=count+a;
		}
		return count;
	}

	static public fun test4():int
	{
		var count=0;
		goto l1;
		count=count+1;
		label l1;
		count=count+2;
		goto l2;
		count=count+3;
		label l2;
		count=count+4;
		return count;
	}

	static public fun test5():int
	{
		var count=1;
		while(count<15)
		{
			count=count*2;
		}

		return count;
	}

	static public fun test6():int
	{
		var count=1;
		do
		{
			count=count+5;
		}while(count<=11);

		return count;
	}
}
